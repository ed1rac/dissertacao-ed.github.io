	keywords = {Access control, access control policy, authorisation, Authorization, Communication system control, Detection algorithms, extensible, extensible intra-access control policy conflict detection, Filtering, Filters, intra-policy conflict detection, Logic programming, matrix algebra, matrix group, Packaging, Permission, policy management, policy specification, reusable, Security}
}

@inproceedings{kim_privacy-aware_2010,
	title = {Privacy-Aware Role Based Access Control Model: Revisited for Multi-Policy Conflict Detection},
	doi = {10.1109/ICISA.2010.5480349},
	shorttitle = {Privacy-Aware Role Based Access Control Model},
	abstract = {Privacy-aware Role-based Access Control (P-{RBAC}) model is one of Role-Based Access Control ({RBAC}) model extensions that has been proposed to help the enforcement of privacy policies. Despite the enhanced privacy protection mechanism by P-{RBAC}, its pair-wise policy conflict detection algorithm has been pointed out as one of its limitations because conflicts within more than two policies are not detected. In this paper, we review and extend P-{RBAC}'s existing conflict detection algorithm so that it can check conflicts in multiple policies. Our review includes the performance comparison on two conflict detection algorithms, triple-policy conflict detection vs. double-policy conflict detection. We find out that as the number of policies increases and the number of same condition variables in the policies increases, the running time complexity gets bigger. Especially for tripe-policy conflict, the probability of triple policy conflict also affects the complexity.},
	eventtitle = {2010 International Conference on Information Science and Applications},
	pages = {1--7},
	booktitle = {2010 International Conference on Information Science and Applications},
	author = {Kim, Y. and Song, E.},
